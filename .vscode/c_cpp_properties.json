                "${workspaceFolder}/include/PR/*",
                "${workspaceFolder}/include/PRinternal/*",
                "${workspaceFolder}/include/sys/*",
                "${workspaceFolder}/src/*",
                "${workspaceFolder}/src/ultralib/src/audio/*",
